<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.survey">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logofast"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SURVEY">
        <activity android:name=".StatisticActivity2"></activity>
        <activity android:name=".StatisticsActivity" />
        <activity android:name=".FeedbackQ" />
        <activity android:name=".ResultDisplay" />
        <activity android:name=".FirstTimeSettingScreen" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Scroll2" />
        <activity android:name=".Scroll1" />
        <activity android:name=".setprofile" />
        <activity android:name=".MainActivity" />

        <service android:name=".MyService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".LocationMonitoringService">
            <intent-filter>
                <action android:name="com.androidexample.screenonoff.AEScreenOnOffService" />
            </intent-filter>
        </service>
    </application>
    <uses-permission android:name="android.permission. ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />

</manifest>